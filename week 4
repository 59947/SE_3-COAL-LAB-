TASK 1: 

org 100h
.data
.code
main proc
    
 
    mov ah, 1h        
    int 21h
    sub al, 30       
    mov bl, al        

    
    mov ah, 1h        
    int 21h
    sub al, 30       
    mov bh, al        

    add bh, 30       
    mov dl, bh
    mov ah, 2h
    int 21h

    add bl, 30       
    mov dl, bl
    mov ah, 2h
    int 21h

    mov ah, 4Ch
    int 21h

main endp

end main




